<div class="background_img"></div>
<br>
<div class="standard">
  <h1>Entries</h1>

  <%= Article.public_count %> entries

  <ul>
    <% @articles.each do |article| %>
      <% unless article.archived? %>
        <li>
          <%= link_to article.title, article %>
        </li>
      <% end %>
    <% end %>
  </ul>
  <br><br>

  <div id="godown">
    <%= link_to "New Article", new_article_path %>
    <br>
    <%= link_to "Home", "/" %>
  </div>
</div>